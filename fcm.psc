Algoritmo fcm_algoritmo
	Definir edad, fcm Como Entero;
	//Paso 1   Ingresar la edad
	Escribir "Algoritmo de Cálculo de FCM";
	Escribir ":):):):):):):):):):):):):):)";
	Escribir "";   // Salto de línea
	Escribir Sin Saltar "Ingresa la edad   ";
	Leer edad;
	
	//Paso 2    fcm = 220 - edad
	fcm <- 220 - edad;
	
	//Paso 3    Desplegar la fcm
	Escribir "La FCM es equivalente a ", fcm;
FinAlgoritmo
